{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport NavBar from \"@/Components/NavBar.vue\";\nimport axios from \"axios\";\nexport default {\n  data() {\n    return {\n      users: [],\n      //users из API\n      dishes: [],\n      //dish из API\n      selectedUser: null,\n      // выбранный user из выпадающего списка\n      selectedDish: null,\n      // выбранное dish из выпадающего списка\n      selectedDate: null,\n      // выбранная дата из input'а с датой\n      dish_posts: []\n    };\n  },\n  mounted() {\n    axios.get('http://127.0.0.1:8000/api/user_view/').then(response => {\n      this.users = response.data;\n      console.log(this.users);\n    }).catch(error => {\n      console.log(error);\n    });\n    axios.get('http://127.0.0.1:8000/api/dish_view/').then(response => {\n      this.dishes = response.data;\n      console.log(this.dishes);\n    }).catch(error => {\n      console.log(error);\n    });\n  },\n  components: {\n    NavBar\n  },\n  methods: {\n    addDishPost() {\n      if (!this.dishes.includes(item => item.dish_name === this.selectedDish)) {\n        this.dish_posts.push({\n          dish_name: this.dishes.find(item => item.dish_name === this.selectedDish)['dish_name'],\n          dish_compose: this.dishes.find(item => item.dish_name === this.selectedDish)['dish_compose'],\n          dish_price: this.dishes.find(item => item.dish_name === this.selectedDish)['dish_price'],\n          pk: this.dishes.find(item => item.dish_name === this.selectedDish)['pk'],\n          dish_count: 1\n        });\n        console.log(this.selectedDish);\n        console.log(!this.dishes.includes(item => item.dish_name === this.selectedDish));\n      }\n    },\n    deleteDishPost(index) {\n      this.dish_posts.splice(index, 1);\n    },\n    incrementCountDish(index) {\n      this.dish_posts[index].dish_count += 1;\n    },\n    decrementCountDish(index) {\n      if (this.dish_posts[index].dish_count >= 2) {\n        this.dish_posts[index].dish_count -= 1;\n      }\n    },\n    sendUsersChoice() {\n      if (this.dish_posts.length > 0 && this.selectedUser != null && this.selectedDate != null) {\n        console.log(this.dish_posts);\n        console.log(this.selectedUser);\n        console.log(this.selectedDate);\n      }\n    }\n  }\n};","map":{"version":3,"names":["NavBar","axios","data","users","dishes","selectedUser","selectedDish","selectedDate","dish_posts","mounted","get","then","response","console","log","catch","error","components","methods","addDishPost","includes","item","dish_name","push","find","dish_compose","dish_price","pk","dish_count","deleteDishPost","index","splice","incrementCountDish","decrementCountDish","sendUsersChoice","length"],"sources":["C:\\projects\\приложение для заказа корпоративной еды\\project\\eat_app_front\\src\\pages\\Order.vue"],"sourcesContent":["<template>\r\n    <NavBar/>\r\n    <h1 class=\"page_category\">Order</h1>\r\n    <form @submit.prevent>\r\n        <div class=\"select_user\">\r\n            <select v-model=\"selectedUser\">\r\n                <option v-for=\"user in users\" v-bind:value=\"user.user_name\">{{user.user_name}}</option>\r\n            </select>\r\n        </div>\r\n\r\n        <div>\r\n            <input type=\"date\" v-model=\"selectedDate\">\r\n        </div>\r\n\r\n        <div class=\"select_dish\">\r\n            <label>Выберите блюдо: </label>\r\n            <select v-model=\"selectedDish\" id=\"dish_item\">\r\n                <option disabled value=\"\">Выберите товар</option>\r\n                <option v-for=\"dish in dishes\" :key=\"dish.pk\" :value=\"dish.dish_name\">\r\n                    {{ dish.dish_name }}\r\n                </option>\r\n            </select>\r\n\r\n            <span v-if=\"selectedDish\">{{ dishes.find(dish => dish.dish_name === selectedDish).dish_price }} ₽</span>\r\n        </div>\r\n\r\n        <button @click=\"addDishPost\">Добавить блюдо</button>\r\n        <div v-for=\"(dish, index) in dish_posts\" :key=\"index\">\r\n\r\n            <div><h3>{{ dish.dish_name }}</h3></div>\r\n\r\n            <div><h4>{{ dish.dish_price }} ₽</h4></div>\r\n\r\n            <div>Кол-во: {{ dish.dish_count }}</div>\r\n\r\n            <button @click=\"incrementCountDish(index)\">+</button>\r\n\r\n            <button @click=\"decrementCountDish(index)\">-</button>\r\n\r\n            <button @click=\"deleteDishPost(index)\">Удалить товар</button>\r\n\r\n        </div>\r\n        <div>\r\n            <button @click=\"sendUsersChoice\">Заказать</button>\r\n        </div>\r\n    </form>\r\n</template>\r\n\r\n<script>\r\nimport NavBar from \"@/Components/NavBar.vue\";\r\nimport axios from \"axios\";\r\n\r\n\r\nexport default {\r\n    data() {\r\n        return {\r\n            users: [], //users из API\r\n            dishes: [], //dish из API\r\n            selectedUser: null, // выбранный user из выпадающего списка\r\n            selectedDish: null, // выбранное dish из выпадающего списка\r\n            selectedDate: null, // выбранная дата из input'а с датой\r\n            dish_posts: [],\r\n        };\r\n    },\r\n\r\n    mounted() {\r\n        axios.get('http://127.0.0.1:8000/api/user_view/')\r\n            .then(response => {\r\n                this.users = response.data\r\n                console.log(this.users)\r\n            })\r\n            .catch(error => {\r\n                console.log(error);\r\n        })\r\n        axios.get('http://127.0.0.1:8000/api/dish_view/')\r\n            .then(response => {\r\n                this.dishes = response.data\r\n                console.log(this.dishes)\r\n            })\r\n            .catch(error => {\r\n                console.log(error)\r\n            })\r\n    },\r\n    components: {NavBar},\r\n    methods: {\r\n        addDishPost() {\r\n            if (!this.dishes.includes(item => item.dish_name === this.selectedDish)) {\r\n                this.dish_posts.push({\r\n                    dish_name: this.dishes.find(item => item.dish_name === this.selectedDish)['dish_name'],\r\n                    dish_compose: this.dishes.find(item => item.dish_name === this.selectedDish)['dish_compose'],\r\n                    dish_price: this.dishes.find(item => item.dish_name === this.selectedDish)['dish_price'],\r\n                    pk: this.dishes.find(item => item.dish_name === this.selectedDish)['pk'],\r\n                    dish_count: 1,\r\n                })\r\n                console.log(this.selectedDish)\r\n                console.log(!this.dishes.includes(item => item.dish_name === this.selectedDish))\r\n            }\r\n        },\r\n        deleteDishPost(index) {\r\n            this.dish_posts.splice(index, 1)\r\n        },\r\n        incrementCountDish(index) {\r\n            this.dish_posts[index].dish_count += 1\r\n        },\r\n        decrementCountDish(index) {\r\n            if (this.dish_posts[index].dish_count >= 2){\r\n                this.dish_posts[index].dish_count -= 1\r\n            }\r\n        },\r\n\r\n        sendUsersChoice() {\r\n            if (this.dish_posts.length > 0 && this.selectedUser != null && this.selectedDate != null){\r\n                console.log(this.dish_posts)\r\n                console.log(this.selectedUser)\r\n                console.log(this.selectedDate)\r\n            }\r\n        },\r\n    }\r\n};\r\n</script>\r\n\r\n<style lang=\"scss\" scoped>\r\n\r\n</style>"],"mappings":";AAiDA,OAAOA,MAAK,MAAO,yBAAyB;AAC5C,OAAOC,KAAI,MAAO,OAAO;AAGzB,eAAe;EACXC,IAAIA,CAAA,EAAG;IACH,OAAO;MACHC,KAAK,EAAE,EAAE;MAAE;MACXC,MAAM,EAAE,EAAE;MAAE;MACZC,YAAY,EAAE,IAAI;MAAE;MACpBC,YAAY,EAAE,IAAI;MAAE;MACpBC,YAAY,EAAE,IAAI;MAAE;MACpBC,UAAU,EAAE;IAChB,CAAC;EACL,CAAC;EAEDC,OAAOA,CAAA,EAAG;IACNR,KAAK,CAACS,GAAG,CAAC,sCAAsC,EAC3CC,IAAI,CAACC,QAAO,IAAK;MACd,IAAI,CAACT,KAAI,GAAIS,QAAQ,CAACV,IAAG;MACzBW,OAAO,CAACC,GAAG,CAAC,IAAI,CAACX,KAAK;IAC1B,CAAC,EACAY,KAAK,CAACC,KAAI,IAAK;MACZH,OAAO,CAACC,GAAG,CAACE,KAAK,CAAC;IAC1B,CAAC;IACDf,KAAK,CAACS,GAAG,CAAC,sCAAsC,EAC3CC,IAAI,CAACC,QAAO,IAAK;