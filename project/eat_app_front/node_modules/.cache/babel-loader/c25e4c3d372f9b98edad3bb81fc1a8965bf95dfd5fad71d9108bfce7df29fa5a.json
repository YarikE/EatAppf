{"ast":null,"code":"import NavBar from \"@/Components/NavBar.vue\";\nimport axios from \"axios\";\nexport default {\n  components: {\n    NavBar\n  },\n  data() {\n    return {\n      users: [],\n      dish_post: [],\n      selectedUser: null,\n      max_dish_count: null,\n      dish_count_choice: 1\n    };\n  },\n  mounted() {\n    axios.get('http://127.0.0.1:8000/api/user_view/').then(response => {\n      this.users = response.data;\n    }).catch(error => {\n      console.log(error);\n    });\n    axios.get('http://127.0.0.1:8000/api/dish_view/').then(response => {\n      this.max_dish_count = response.data.length;\n    }).catch(error => {\n      console.log(error);\n    });\n  },\n  methods: {\n    incrementCountDish() {\n      if (this.dish_count_choice < this.max_dish_count) {\n        this.dish_count_choice += 1;\n      }\n    },\n    decrementCountDish() {\n      if (this.dish_count_choice > 1) {\n        this.dish_count_choice -= 1;\n      }\n    },\n    get_random_order() {\n      while (this.dish_post.length !== this.dish_count_choice) {\n        axios.post('http://127.0.0.1:8000/api/random_dish/').then(response => {\n          if (!this.dish_post.some(response.data)) {\n            this.dish_post.append(response.data);\n          }\n          console.log(response.data);\n        }).catch(error => {\n          console.log(error);\n        });\n      }\n      console.log(this.dish_post);\n    }\n  }\n};","map":{"version":3,"names":["NavBar","axios","components","data","users","dish_post","selectedUser","max_dish_count","dish_count_choice","mounted","get","then","response","catch","error","console","log","length","methods","incrementCountDish","decrementCountDish","get_random_order","post","some","append"],"sources":["C:\\projects\\приложение для заказа корпоративной еды\\project\\eat_app_front\\src\\pages\\Lucky.vue"],"sourcesContent":["<template>\r\n  <NavBar/>\r\n    <h1 class=\"page_category\">Режим \"Мне повезет\"</h1>\r\n    <div class=\"select_user\">\r\n        <select v-model=\"selectedUser\">\r\n            <option v-for=\"user in users\" v-bind:value=\"user.user_name\">{{user.user_name}}</option>\r\n        </select>\r\n    </div>\r\n\r\n    <div class=\"dish_count\">\r\n        <button @click=\"incrementCountDish(this.dish_count_choice)\">+</button>\r\n        <p>{{ this.dish_count_choice }}</p>\r\n        <button @click=\"decrementCountDish(this.dish_count_choice)\">-</button>\r\n    </div>\r\n\r\n    <button @click=\"get_random_order\">Мне повезет</button>\r\n</template>\r\n\r\n<script>\r\nimport NavBar from \"@/Components/NavBar.vue\";\r\nimport axios from \"axios\";\r\n\r\nexport default {\r\n    components: {NavBar},\r\n    data() {\r\n        return {\r\n            users: [],\r\n            dish_post: [],\r\n            selectedUser: null,\r\n            max_dish_count: null,\r\n            dish_count_choice: 1,\r\n        }\r\n    },\r\n    mounted() {\r\n        axios.get('http://127.0.0.1:8000/api/user_view/')\r\n            .then(response => {\r\n                this.users = response.data\r\n            })\r\n            .catch(error => {\r\n                console.log(error);\r\n            })\r\n        axios.get('http://127.0.0.1:8000/api/dish_view/')\r\n            .then(response => {\r\n                this.max_dish_count = response.data.length\r\n            })\r\n            .catch(error => {\r\n                console.log(error)\r\n            })\r\n    },\r\n    methods: {\r\n        incrementCountDish(){\r\n            if (this.dish_count_choice < this.max_dish_count){\r\n                this.dish_count_choice += 1;\r\n            }\r\n        },\r\n        decrementCountDish() {\r\n            if (this.dish_count_choice > 1){\r\n                this.dish_count_choice -= 1;\r\n          }\r\n        },\r\n        get_random_order(){\r\n            while (this.dish_post.length !== this.dish_count_choice){\r\n                axios.post('http://127.0.0.1:8000/api/random_dish/')\r\n                    .then(response => {\r\n                        if (!this.dish_post.some(response.data)){\r\n                            this.dish_post.append(response.data)\r\n                        }\r\n                        console.log(response.data)\r\n                    })\r\n                    .catch(error => {\r\n                        console.log(error)\r\n                    })\r\n            }\r\n            console.log(this.dish_post)\r\n\r\n        }\r\n    },\r\n}\r\n</script>\r\n\r\n<style lang=\"scss\" scoped>\r\n\r\n</style>"],"mappings":"AAmBA,OAAOA,MAAK,MAAO,yBAAyB;AAC5C,OAAOC,KAAI,MAAO,OAAO;AAEzB,eAAe;EACXC,UAAU,EAAE;IAACF;EAAM,CAAC;EACpBG,IAAIA,CAAA,EAAG;IACH,OAAO;MACHC,KAAK,EAAE,EAAE;MACTC,SAAS,EAAE,EAAE;MACbC,YAAY,EAAE,IAAI;MAClBC,cAAc,EAAE,IAAI;MACpBC,iBAAiB,EAAE;IACvB;EACJ,CAAC;EACDC,OAAOA,CAAA,EAAG;IACNR,KAAK,CAACS,GAAG,CAAC,sCAAsC,EAC3CC,IAAI,CAACC,QAAO,IAAK;MACd,IAAI,CAACR,KAAI,GAAIQ,QAAQ,CAACT,IAAG;IAC7B,CAAC,EACAU,KAAK,CAACC,KAAI,IAAK;MACZC,OAAO,CAACC,GAAG,CAACF,KAAK,CAAC;IACtB,CAAC;IACLb,KAAK,CAACS,GAAG,CAAC,sCAAsC,EAC3CC,IAAI,CAACC,QAAO,IAAK;MACd,IAAI,CAACL,cAAa,GAAIK,QAAQ,CAACT,IAAI,CAACc,MAAK;IAC7C,CAAC,EACAJ,KAAK,CAACC,KAAI,IAAK;MACZC,OAAO,CAACC,GAAG,CAACF,KAAK;IACrB,CAAC;EACT,CAAC;EACDI,OAAO,EAAE;IACLC,kBAAkBA,CAAA,EAAE;MAChB,IAAI,IAAI,CAACX,iBAAgB,GAAI,IAAI,CAACD,cAAc,EAAC;QAC7C,IAAI,CAACC,iBAAgB,IAAK,CAAC;MAC/B;IACJ,CAAC;IACDY,kBAAkBA,CAAA,EAAG;MACjB,IAAI,IAAI,CAACZ,iBAAgB,GAAI,CAAC,EAAC;QAC3B,IAAI,CAACA,iBAAgB,IAAK,CAAC;MACjC;IACF,CAAC;IACDa,gBAAgBA,CAAA,EAAE;MACd,OAAO,IAAI,CAAChB,SAAS,CAACY,MAAK,KAAM,IAAI,CAACT,iBAAiB,EAAC;QACpDP,KAAK,CAACqB,IAAI,CAAC,wCAAwC,EAC9CX,IAAI,CAACC,QAAO,IAAK;UACd,IAAI,CAAC,IAAI,CAACP,SAAS,CAACkB,IAAI,CAACX,QAAQ,CAACT,IAAI,CAAC,EAAC;YACpC,IAAI,CAACE,SAAS,CAACmB,MAAM,CAACZ,QAAQ,CAACT,IAAI;UACvC;UACAY,OAAO,CAACC,GAAG,CAACJ,QAAQ,CAACT,IAAI;QAC7B,CAAC,EACAU,KAAK,CAACC,KAAI,IAAK;UACZC,OAAO,CAACC,GAAG,CAACF,KAAK;QACrB,CAAC;MACT;MACAC,OAAO,CAACC,GAAG,CAAC,IAAI,CAACX,SAAS;IAE9B;EACJ;AACJ"},"metadata":{},"sou