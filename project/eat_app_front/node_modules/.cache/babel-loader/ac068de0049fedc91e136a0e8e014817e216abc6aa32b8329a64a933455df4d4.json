{"ast":null,"code":"export default {\n  data() {\n    return {\n      posts: [{\n        title: 'Пост 1',\n        items: ['Товар 1', 'Товар 2', 'Товар 3'],\n        selectedItem: '',\n        price: '',\n        likes: 0\n      }, {\n        title: 'Пост 2',\n        items: ['Товар 4', 'Товар 5', 'Товар 6'],\n        selectedItem: '',\n        price: '',\n        likes: 0\n      }]\n    };\n  },\n  methods: {\n    deletePost(index) {\n      this.posts.splice(index, 1);\n    },\n    likePost(index, value) {\n      this.posts[index].likes += value;\n    },\n    logValues() {\n      console.log(this.posts);\n    }\n  },\n  watch: {\n    'posts.selectedItem': function (newVal, oldVal) {\n      // Здесь можно реализовать логику для подтягивания цены товара\n      // newVal - новое значение выбранного элемента\n      // oldVal - старое значение выбранного элемента\n    }\n  }\n};","map":{"version":3,"names":["data","posts","title","items","selectedItem","price","likes","methods","deletePost","index","splice","likePost","value","logValues","console","log","watch","posts.selectedItem","newVal","oldVal"],"sources":["C:\\projects\\приложение для заказа корпоративной еды\\project\\eat_app_front\\src\\pages\\History.vue"],"sourcesContent":["<template>\r\n    <div>\r\n        <h2>Список постов</h2>\r\n        <div v-for=\"(post, index) in posts\" :key=\"index\">\r\n            <h3>{{ post.title }}</h3>\r\n            <select v-model=\"post.selectedItem\">\r\n                <option v-for=\"item in post.items\" :value=\"item\">{{ item }}</option>\r\n            </select>\r\n            <span>{{ post.price }}</span>\r\n            <button @click=\"deletePost(index)\">Удалить пост</button>\r\n            <button @click=\"likePost(index, 1)\">+</button>\r\n            <span>{{ post.likes }}</span>\r\n            <button @click=\"likePost(index, -1)\">-</button>\r\n        </div>\r\n        <button @click=\"logValues\">Вывести данные в консоль</button>\r\n    </div>\r\n</template>\r\n\r\n<script>\r\nexport default {\r\n    data() {\r\n        return {\r\n            posts: [\r\n                {\r\n                    title: 'Пост 1',\r\n                    items: ['Товар 1', 'Товар 2', 'Товар 3'],\r\n                    selectedItem: '',\r\n                    price: '',\r\n                    likes: 0\r\n                },\r\n                {\r\n                    title: 'Пост 2',\r\n                    items: ['Товар 4', 'Товар 5', 'Товар 6'],\r\n                    selectedItem: '',\r\n                    price: '',\r\n                    likes: 0\r\n                }\r\n            ]\r\n        }\r\n    },\r\n    methods: {\r\n        deletePost(index) {\r\n            this.posts.splice(index, 1)\r\n        },\r\n        likePost(index, value) {\r\n            this.posts[index].likes += value\r\n        },\r\n        logValues() {\r\n            console.log(this.posts)\r\n        }\r\n    },\r\n    watch: {\r\n        'posts.selectedItem': function(newVal, oldVal) {\r\n            // Здесь можно реализовать логику для подтягивания цены товара\r\n            // newVal - новое значение выбранного элемента\r\n            // oldVal - старое значение выбранного элемента\r\n        }\r\n    }\r\n}\r\n</script>\r\n\r\n<style lang=\"scss\" scoped>\r\n\r\n</style>"],"mappings":"AAmBA,eAAe;EACXA,IAAIA,CAAA,EAAG;IACH,OAAO;MACHC,KAAK,EAAE,CACH;QACIC,KAAK,EAAE,QAAQ;QACfC,KAAK,EAAE,CAAC,SAAS,EAAE,SAAS,EAAE,SAAS,CAAC;QACxCC,YAAY,EAAE,EAAE;QAChBC,KAAK,EAAE,EAAE;QACTC,KAAK,EAAE;MACX,CAAC,EACD;QACIJ,KAAK,EAAE,QAAQ;QACfC,KAAK,EAAE,CAAC,SAAS,EAAE,SAAS,EAAE,SAAS,CAAC;QACxCC,YAAY,EAAE,EAAE;QAChBC,KAAK,EAAE,EAAE;QACTC,KAAK,EAAE;MACX;IAER;EACJ,CAAC;EACDC,OAAO,EAAE;IACLC,UAAUA,CAACC,KAAK,EAAE;MACd,IAAI,CAACR,KAAK,CAACS,MAAM,CAACD,KAAK,EAAE,CAAC;IAC9B,CAAC;IACDE,QAAQA,CAACF,KAAK,EAAEG,KAAK,EAAE;MACnB,IAAI,CAACX,KAAK,CAACQ,KAAK,CAAC,CAACH,KAAI,IAAKM,KAAI;IACnC,CAAC;IACDC,SAASA,CAAA,EAAG;MACRC,OAAO,CAACC,GAAG,CAAC,IAAI,CAACd,KAAK;IAC1B;EACJ,CAAC;EACDe,KAAK,EAAE;IA